<html>
    <head>
        <meta charset="utf-8">
        <title>게시글</title>
    </head>
    <body>
        <h2>게시글 리스트</h2>
        <table border='1'>
            <colgroup>
                <col width='60'><col width='60'><col width='200'>
            </colgroup>
            <thead>
                <tr>
                    <th>글번호</th>
                    <th>글제목</th>
                    <th>글내용</th>
                    <th>작성날짜</th>
                </tr>
            </thead>
            <tbody>
                <% for(i = 0; i < list.length; i++) { %>
                    <% date = list[i].date.toString().split(' ') %>
                    <% console.log(date[0]) %>
                <tr>
                    <td><%=list[i].id %></td>
                    <td><%=list[i].title %></td>
                    <td><%=list[i].content %></td>
                    <td style="font-size:3px"><%=date[3]+"."+date[1]+"."+date[2]+"("+date[0]+")"+" "+date[4] %></td>
                </tr>
                <% } %>
            </tbody>
        </table boader='1'>
        
        <button onclick="location.href='../write'">글쓰기</button>
        <button onclick="location.href='../delete'">글삭제</button>
        <button onclick="location.href='../update'">글수정</button>
    </body>
</html>